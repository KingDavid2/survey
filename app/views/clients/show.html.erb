<h1>
  Dialogos
</h1>
<div class='row'>
  <% @surveys.order(slug: :asc).each do | survey | %>
    <div class='col-md-4 col-sm-6'>
      <div class="card text-center">
      <h5 class="card-header"><%= survey.name %></h5>
      <div class="card-body">
          <h5 class="card-title"></h5>
          <%= link_to 'Ir', "/#{@client.slug}/#{survey.slug}", class: 'btn btn-primary' %>
        </div>
      </div>
    </div>
  <% end %>
</div>
